{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "singleAttributePerLine": true,
  "trailingComma": "none",
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-organize-attributes"
  ],
  "attributeGroups": [
    "^class$",
    "^(id|name)$",
    "$DEFAULT",
    "^aria-"
  ]
}